<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="@dimen/_2sdp" android:color="@color/white" />
            <solid android:color="#99cc00" />
            <corners android:topLeftRadius="@dimen/_8sdp"
                android:topRightRadius="@dimen/_8sdp"/>

        </shape>
    </item>

    <item android:top="@dimen/_2sdp" android:left="@dimen/_2sdp" android:right="@dimen/_2sdp">
        <shape
            android:shape="rectangle">
            <stroke android:width="@dimen/_2sdp" android:color="#99cc00" />
            <solid android:color="#00000000" />
            <corners android:topLeftRadius="@dimen/_8sdp"
                android:topRightRadius="@dimen/_8sdp"/>

        </shape>
    </item>
</layer-list>
