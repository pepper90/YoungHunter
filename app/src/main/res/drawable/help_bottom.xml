<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="@dimen/_1sdp" android:color="@color/white" />
            <solid android:color="@color/olive_2" />
            <corners android:bottomLeftRadius="@dimen/_8sdp"
                android:bottomRightRadius="@dimen/_8sdp"/>

        </shape>
    </item>

    <item android:bottom="@dimen/_1sdp" android:left="@dimen/_1sdp" android:right="@dimen/_1sdp">
        <shape
            android:shape="rectangle">
            <stroke android:width="@dimen/_1sdp" android:color="@color/olive_2" />
            <solid android:color="#00000000" />
            <corners android:bottomLeftRadius="@dimen/_8sdp"
                android:bottomRightRadius="@dimen/_8sdp"/>

        </shape>
    </item>




</layer-list>
