<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jpdevzone.younghunter">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.YoungHunter"
        tools:ignore="AllowBackup,LockedOrientationActivity">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-7588987461083278~7432990397" />

        <activity
            android:name="com.jpdevzone.younghunter.QuizQuestionsExam"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.QuizQuestionsAnimals"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.QuizQuestionsHuntingLaw"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.QuizQuestionsHuntingMethods"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.QuizQuestionsGameManagement"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.QuizQuestionsGuns"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.QuizQuestionsViruses"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.Help"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.FinishQuiz"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.QuizQuestionsDogs"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.Dashboard"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name="com.jpdevzone.younghunter.MainActivity"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>